{
  "name": "dojo-tools",
  "version": "1.0.0",
  "description": "Tools for MCLJSUG",
  "engines": {
    "node": "8.12.0"
  },
  "scripts": {
    "clean": "rm -rf ./target ./.shadow-cljs ./public/js",
    "watch-app": "npm run clean && shadow-cljs watch app",
    "dev": "concurrently --kill-others \"nodemon --watch server ./server/index.js\" \"npm run watch-app\"",
    "compile": "shadow-cljs release app",
    "server": "NODE_ENV=production node ./server/index.js"
  },
  "author": "@thachenko1503",
  "license": "MIT",
  "dependencies": {
    "create-react-class": "15.6.3",
    "dotenv": "7.0.0",
    "ejs": "2.6.1",
    "express": "4.16.3",
    "firebase": "5.4.2",
    "moment": "2.22.2",
    "morgan": "1.9.1",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-fela": "10.2.1",
    "shadow-cljs": "2.8.13",
    "react-datepicker": "2.2.0"
  }
}
